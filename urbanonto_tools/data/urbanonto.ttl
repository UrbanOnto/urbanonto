        "monument"@en,
        "statue"@en,
        "monument"@fr,
        "monumentum"@la,
        "monument"@pl,
        "obelisk"@pl,
        "posąg"@pl,
        "statua"@pl,
        "pamiatnik"@ru ;
    ns1:hiddenLabel "rzeźba"@pl .


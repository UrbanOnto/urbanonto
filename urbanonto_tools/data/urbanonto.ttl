            owl:someValuesFrom urbanonto:object_type_35 ],
        [ a owl:Restriction ;
            owl:onProperty cidoc_crm:P46_is_composed_of ;
            owl:someValuesFrom urbanonto:object_type_13 ],
        <https://purl.org/bdot10k#object_120> ;
    ns1:altLabel "Lehranstalt"@de,
        "Schule"@de,
        "school"@en,
        "école"@fr,
        "academia"@la,
        "schola"@la,
        "zakład naukowy"@pl,
        "ucziliszcze"@ru ;
    ns1:hiddenLabel "szkoła wyższa"@pl,
        "uniwersytet"@pl .


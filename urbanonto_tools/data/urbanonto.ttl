    rdfs:domain cidoc_crm:E55_Type ;
    rdfs:range cidoc_crm:E71_Man-Made_Thing .

cidoc_crm:P104_is_subject_to a owl:ObjectProperty ;
    rdfs:label "Gegenstand von"@de,
        "υπόκειται σε"@el,
        "is subject to"@en,
        "est sujet à"@fr,
        "está sujeito à"@pt,
        "является объектом для"@ru,
        "受制於"@zh ;
    rdfs:comment """This property links a particular E72 Legal Object to the instances of E30 Right to which it is subject.\r
The Right is held by an E39 Actor as described by P75 possesses (is possessed by).\r
"""@en ;
    rdfs:domain cidoc_crm:E72_Legal_Object ;
    rdfs:range cidoc_crm:E30_Right .

cidoc_crm:P104i_applies_to a owl:ObjectProperty ;
    rdfs:label "findet Anwendung auf"@de,
